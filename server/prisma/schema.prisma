generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @id @default(autoincrement())
  createdAt  DateTime @default(now())
  first_name String
  last_name  String
  email      String   @unique
  username   String   @unique
  password   String
  roles      Roles[]  @default([USER])
}

enum Roles {
  USER
  ADMIN
  SUPER_ADMIN
}
